{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "google"
    ],
    "plugins": ["import"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "semi": [2, "always"],
      "quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }]
    },
    "ignorePatterns": ["*RWXLoader.js*"],
    "globals": {
      "require": true
    }
}
